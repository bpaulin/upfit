{
    "name": "bpaulin/upfit",
    "license": "MIT",
    "type": "project",
    "description": "Bpaulin Upfit",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "jquery/jquery": "*",
        "jquery-ui/js": "*",
        "bootstrap/js": "*",
        "phing/phing": "dev-master",
        "doctrine/data-fixtures": "dev-master@dev",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/migrations": "dev-master@dev",
        "doctrine/doctrine-migrations-bundle": "dev-master@dev",
        "friendsofsymfony/user-bundle": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "dev-master",
        "phpmd/phpmd" : "1.4.*",
        "sebastian/phpcpd": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "behat/behat":  "2.4.*@dev",
        "behat/mink":   "1.4.*@dev",
        "behat/symfony2-extension":      "*@dev",
        "behat/mink-extension":          "*@dev",
        "behat/mink-browserkit-driver":  "*@dev",
        "behat/mink-selenium2-driver" :   "*@dev",
        "vipsoft/doctrine-data-fixtures-extension": "*",
        "selenium/standalone-server"  : "*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-latest.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery-ui/js",
                "version": "1",
                "dist": {
                    "url": "http://code.jquery.com/ui/1.10.3/jquery-ui.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "bootstrap/js",
                "version": "2.3.2",
                "dist": {
                    "url": "http://netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/js/bootstrap.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "selenium/standalone-server",
                "version": "2.33",
                "dist": {
                    "url": "http://selenium.googlecode.com/files/selenium-server-standalone-2.33.0.jar",
                    "type": "file"
                }
            }
        }
    ]
}
