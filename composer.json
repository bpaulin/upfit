{
    "name": "bpaulin/upfit",
    "license": "MIT",
    "type": "project",
    "description": "Bpaulin Upfit",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php"                                 : ">=5.3.3",
        "symfony/symfony"                     : "~2.3@stable",
        "doctrine/orm"                        : "~2.2@stable",
        "doctrine/doctrine-bundle"            : "~1.2@stable",
        "twig/extensions"                     : "~1.0@stable",
        "symfony/assetic-bundle"              : "~2.3@stable",
        "symfony/swiftmailer-bundle"          : "~2.3@stable",
        "symfony/monolog-bundle"              : "~2.3@stable",
        "sensio/distribution-bundle"          : "~2.3@stable",
        "sensio/framework-extra-bundle"       : "~2.3@stable",
        "sensio/generator-bundle"             : "~2.3@stable",
        "incenteev/composer-parameter-handler": "~2.0@stable",
        "doctrine/data-fixtures"              : "dev-master@dev",
        "doctrine/doctrine-fixtures-bundle"   : "dev-master@dev",
        "doctrine/doctrine-migrations-bundle" : "dev-master@dev",
        "friendsofsymfony/user-bundle"        : "*",
        "jms/translation-bundle"              : "1.1.*",
        "jms/i18n-routing-bundle"             : "1.1.*@dev",
        "jms/di-extra-bundle"                 : "1.4.*@dev"
    },
    "require-dev": {
        "sensiolabs/security-checker"             : "*",
        "twig/extensions"                         : "1.0.*@dev",
        "squizlabs/php_codesniffer"               : "dev-master",
        "zetacomponents/console-tools"            : "dev-master",
        "phploc/phploc"                           : "dev-master",
        "phpmd/phpmd"                             : "1.4.*",
        "sebastian/phpcpd"                        : "dev-master",
        "phpunit/phpunit"                         : "3.7.*",
        "behat/behat"                             : "2.4.*@dev",
        "behat/mink"                              : "1.4.*@dev",
        "behat/symfony2-extension"                : "*@dev",
        "behat/mink-extension"                    : "*@dev",
        "behat/mink-browserkit-driver"            : "*@dev",
        "behat/mink-selenium2-driver"             : "*@dev",
        "vipsoft/doctrine-data-fixtures-extension": "*",
        "selenium/standalone-server"              : "*",
        "leafo/lessphp"                           : "0.3.*@dev",
        "twitter/bootstrap"                       : "*",
        "components/font-awesome"                 : "dev-master@dev",
        "components/jqueryui"                     : "1.*",
        "components/jquery"                       : "1.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "vendor/components"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "selenium/standalone-server",
                "version": "2.33",
                "dist": {
                    "url": "http://selenium.googlecode.com/files/selenium-server-standalone-2.33.0.jar",
                    "type": "file"
                }
            }
        }
    ]
}
