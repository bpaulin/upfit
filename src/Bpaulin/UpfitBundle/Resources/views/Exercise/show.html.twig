{% extends "BpaulinUpfitBundle:Crud:show.html.twig" %}
{%
    set actions = {
        'Back to the list' : path('admin_exercise'),
        'Edit' : path('admin_exercise_edit', { 'id': entity.id }),
        'Delete' : path('admin_exercise_delete', { 'id': entity.id }),
    }
%}
{%
    set fields = {
        'Id' : entity.id,
        'Name' : entity.name,
    }
%}

{% block h1 %}Exercise <small>{{ entity.name }}</small>{% endblock %}
