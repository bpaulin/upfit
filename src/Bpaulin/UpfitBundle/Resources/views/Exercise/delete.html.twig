{% extends "BpaulinUpfitBundle:Crud:delete.html.twig" %}
{%
    set breadcrumbs = {
        'Exercises' : {
            'route': path('admin_exercise'),
            'icon': 'list',
        },
        (entity.name) : {
            'route': path('admin_exercise_show', { 'id': entity.id }),
        },
        'Delete' : {
            'route': path('admin_exercise_delete', { 'id': entity.id }),
            'icon': 'minus',
        },
    }
%}
{%
    set action = path('admin_exercise_delete_confirm', { 'id': entity.id })
%}

{% block h1 %}Exercise <small>{{ entity.name }}</small>{% endblock %}
