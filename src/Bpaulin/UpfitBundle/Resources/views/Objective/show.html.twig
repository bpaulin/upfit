{% extends "BpaulinUpfitBundle:Crud:show.html.twig" %}
{%
    set breadcrumbs = {
        ('Member'|trans) : {
            'route': path('upfit_member'),
            'icon': 'gamepad',
        },
        ('Muscles'|trans) : {
            'route': path('member_muscle'),
            'icon': 'list',
        },
        (entity.name) : {
            'route': path('member_session_workout', { 'id': entity.id }),
        },
    }
%}
{%
    set fields = {
        ('id'|trans) : entity.id,
        ('name'|trans) : entity.name,
    }
%}

{% block h1 %}{% trans %}Muscle{% endtrans %} <small>{{ entity.name }}</small>{% endblock %}
